{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "delivery_adress": {
      "type": "text"
    },
    "picture": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::picture.picture",
      "inversedBy": "order"
    },
    "full_name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone_number": {
      "type": "string"
    },
    "invoice_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "status": {
      "type": "string",
      "regex": "created|sent|paid|expired",
      "default": "created"
    },
    "invoice_url": {
      "type": "string",
      "required": true
    }
  }
}
