{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "deliveryAddress": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "picture": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::picture.picture",
      "inversedBy": "order"
    },
    "fullName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "phoneNumber": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "invoiceId": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "status": {
      "type": "string",
      "regex": "created|sent|paid|expired",
      "default": "created",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "invoiceUrl": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
